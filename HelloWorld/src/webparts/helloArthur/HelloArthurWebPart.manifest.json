  "requiresCustomScript": false,
  "supportedHosts": ["SharePointWebPart"],

  "preconfiguredEntries": [{
    "groupId": "5c03119e-3074-46fd-976b-c60198311f70", // Other
    "group": { "default": "Other" },
    "title": { "default": "HelloArthur" },
    "description": { "default": "The first SP webpart Sez has ever made. Has a HHGttG feel." },
    "officeFabricIconFontName": "Page",
    "properties": {
      "character": "Arthur Dent",
      "description": "Wears a dressing gown and drinks tea. Overwhelmed by the galaxy and freaks out at most things.",
      "motto": "So this is it, we're going to die?",
      "isPanicking": true,
      "equippedItem": "Towel",
      "hasTea": false
    }
  }]
}
